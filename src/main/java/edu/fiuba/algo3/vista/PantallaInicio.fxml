<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="512.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.fiuba.algo3.controlador.PantallaInicioControlador">
   <children>
      <ImageView fx:id="FondoInicio" fitHeight="640.0" fitWidth="800.0" layoutX="-64.0" layoutY="-64.0" pickOnBounds="true" preserveRatio="true" scaleX="0.8" scaleY="0.8" scaleZ="0.8">
         <image>
            <Image url="@../../../../../../../docs/imagenes/inicio/globo_teg_3.png" />
         </image>
      </ImageView>
      <Button fx:id="BotonEmpezar" contentDisplay="RIGHT" layoutX="165.0" layoutY="268.0" mnemonicParsing="false" onAction="#empezarJuego" prefHeight="35.0" prefWidth="182.0" scaleX="1.5" scaleY="1.7" scaleZ="1.5" text="Empezar partida">
         <font>
            <Font name="Algerian" size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="21.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../resources/iconos/start_48px.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="botonSalir" contentDisplay="RIGHT" layoutX="339.0" layoutY="401.0" mnemonicParsing="false" onAction="#salir" prefHeight="26.0" prefWidth="95.0" scaleX="1.5" scaleY="1.7" scaleZ="1.5" text="Salir">
         <font>
            <Font name="Algerian" size="14.0" />
         </font>
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../resources/iconos/exit_50px.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</Pane>
